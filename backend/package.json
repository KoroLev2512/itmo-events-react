    "@fastify/static": "^6.10.2",
    "@nestjs/common": "^10.1.2",
    "@nestjs/config": "^3.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/platform-express": "^10.1.2",
    "@nestjs/platform-fastify": "^10.1.3",
    "@nestjs/sequelize": "^10.0.0",
    "@nestjs/swagger": "^7.1.2",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
