    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.5",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/core": "^10.1.2",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/swagger": "^7.1.2",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
